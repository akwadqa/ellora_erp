 "html": "{% if letter_head %}\n    <div style=\"margin-bottom: 20px;\">{{ letter_head }}</div>\n{% endif %}\n\n<h1 class=\"text-center\" style=\"width: 100%; font-weight: bold;\">JOURNAL VOUCHER</h1>\n\n<table style=\"width: 100%; font-size: 13px; font-weight: bold; margin-top: 20px;\">\n    <tbody>\n        <tr>\n            <td style=\"width: 60%;\">\n                <span style=\"background-color: #FFC300; display: inline-block; padding: 5px 40px;\">{{ doc.company }}</span>\n            </td>\n            <td>\n                Voucher No:\n            </td>\n            <td class=\"text-right\">\n                {{ doc.name }}\n            </td>\n        </tr>\n        <tr>\n            <td style=\"width: 60%;\">\n                Remarks: {{ doc.user_remark }}\n            </td>\n            <td>\n                Voucher Date:\n            </td>\n            <td class=\"text-right\">\n                {{ doc.posting_date }}\n            </td>\n        </tr>\n    </tbody>\n</table>\n\n<table style=\"width: 100%; margin-top: 50px;\">\n    <thead style=\"font-size: 14px; font-weight: bold; background-color: gray;\">\n        <tr>\n            <th style=\"color: white; border-radius: 5px 0 0 5px; width: 5%;\">\n                Slno\n            </th>\n            <th style=\"color: white; width: 10%;\">\n                AccID\n            </th>\n            <th style=\"color: white; width: 40%;\">\n                Ledger Description\n            </th>\n            <th style=\"color: white; width: 10%;\">\n                Debit\n            </th>\n            <th style=\"color: white; width: 10%;\">\n                Credit\n            </th>\n            <th style=\"color: white; border-radius: 0 5px 5px 0; width: 25%;\">\n                Narration\n            </th>\n        </tr>\n    </thead>\n    <tbody style=\"font-size: 14px;\">\n        {%- for row in doc.accounts -%}\n            <tr>\n                <td>\n                    {{ loop.index }}\n                </td>\n                <td>\n                    {{ frappe.db.get_value(\"Account\", row.account, \"account_number\") }}\n                </td>\n                <td>\n                    {{ row.account }}\n                </td>\n                <td>\n                    {{ '{:,.2f}'.format(row.debit_in_account_currency) }} \n                </td>\n                <td>\n                    {{ '{:,.2f}'.format(row.credit_in_account_currency) }}\n                </td>\n                <td>\n                    {{ row.user_remark }}\n                </td>\n            </tr>\n        {%- endfor -%}\n        <tr style=\"border-top: 1px solid black;\">\n            <td>\n                \n            </td>\n            <td>\n                \n            </td>\n            <td>\n                \n            </td>\n            <td style=\"font-weight: bold;\">\n                {{ '{:,.2f}'.format(doc.total_debit) }}\n            </td>\n            <td style=\"font-weight: bold;\">\n                {{ '{:,.2f}'.format(doc.total_credit) }}\n            </td>\n            <td>\n                \n            </td>\n        </tr>\n    </tbody>\n</table>\n\n<table style=\"width: 100%; font-size: 12px; font-weight: bold; border-top: 2px solid gray; margin-top: 50px;\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td style=\"text-align: center;\">Prepared By</td>\n\t\t\t<td style=\"text-align: center;\">Checked By</td>\n\t\t\t<td style=\"text-align: center;\">Approved By</td>\n\t\t</tr>\n\t</tbody>\n</table>",
